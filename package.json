{
  "name": "gamegoldnode",
  "version": "5.0.1",
  "description": "Vallnet Node",
  "main": "index.js",
  "scripts": {
    "start": "node index.js --genesis --workers=false --network=testnet --dev-mode=false --mining=false --debug=true --log-file=true",
    "master": "node index.js --network=main --mining=false --workers=false --bip150=false --bip151=false",
    "slaver": "node index.js --network=main --mining=false --workers=false --bip150=false --bip151=false --password=bookman --keystore=main-bookman-1.keystore --port-offset=10 --prefix=~/.gamegold/slaver",
    "slaver2": "node index.js --network=main --mining=false --workers=false --bip150=false --bip151=false --password=bookman --keystore=main-bookman-2.keystore --port-offset=20 --prefix=~/.gamegold/slaver2",
    "cli-testnet": "node bin/cli --port=2102",
    "cli": "node bin/cli --port=2002"
  },
  "bin": {
    "gg": "./bin/cli-testnet",
    "vc": "./bin/cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bookmansoft/gamegoldnode.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bookmansoft/gamegoldnode/issues"
  },
  "homepage": "https://github.com/bookmansoft/gamegoldnode#readme",
  "dependencies": {
    "body-parser": "1.20.1",
    "cross-env": "5.1.3",
    "express": "4.18.2",
    "gamegold": "^5.0.1",
    "gamerpc": "^7.0.0",
    "n64": "0.2.10",
    "node-fetch": "2.6.1",
    "socket.io": "4.2.0",
    "uuid": "3.3.2"
  },
  "optionalDependencies": {
    "kafkajs": "1.15.0"
  }
}
