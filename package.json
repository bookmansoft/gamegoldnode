  },
  "homepage": "https://github.com/bookmansoft/gamegoldnode#readme",
  "dependencies": {
    "cross-env": "^5.1.3",
    "body-parser": "1.19.0",
    "express": "4.17.0",
    "gamegold": "^2.5.10",
    "gamerpc": "^6.0.5",
    "kafkajs": "^1.15.0",
    "n64": "0.2.10",
    "node-fetch": "^2.6.1",
